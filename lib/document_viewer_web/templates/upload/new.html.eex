<section class="row">
  <div class="column">
    <h2>Upload</h2>

    <%= form_for @conn, Routes.upload_path(@conn, :create), [multipart: :true], fn f -> %>
      <%= file_input f, :file, accept: "image/*" %>
      <br>
      <button class="btn btn-primary" type="submit">Upload</button>
    <% end %>
  </div>
</section>
